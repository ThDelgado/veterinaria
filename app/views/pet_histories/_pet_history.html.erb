<div id="<%= dom_id pet_history %>">
  <p>
    <strong>Weight:</strong>
    <%= pet_history.weight %>
  </p>

  <p>
    <strong>Height:</strong>
    <%= pet_history.height %>
  </p>

  <p>
    <strong>Visit date:</strong>
    <%= pet_history.visit_date %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= pet_history.description %>
  </p>

  <p>
    <strong>Identificacion:</strong>
    <%= pet_history.pet_id %>
  </p>


</div>
